{
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022", "dom"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "paths": {
      "@prodgenie/libs/constant": ["libs/constant/src/index.ts"],
      "@prodgenie/libs/constant/*": ["libs/constant/src/*"],
      "@prodgenie/libs/db": ["libs/db/src/index.ts"],
      "@prodgenie/libs/db/*": ["libs/db/src/*"],
      "@prodgenie/libs/frontend-services": [
        "libs/frontend-services/src/index.ts"
      ],
      "@prodgenie/libs/frontend-services/*": ["libs/frontend-services/src/*"],
      "@prodgenie/libs/prisma": ["libs/prisma/src/index.ts"],
      "@prodgenie/libs/prisma/*": ["libs/prisma/src/*"],
      "@prodgenie/libs/queues": ["libs/queues/src/index.ts"],
      "@prodgenie/libs/queues/*": ["libs/queues/src/*"],
      "@prodgenie/libs/redis": ["libs/redis/src/index.ts"],
      "@prodgenie/libs/redis/*": ["libs/redis/src/*"],
      "@prodgenie/libs/schema": ["libs/schema/src/index.ts"],
      "@prodgenie/libs/schema/*": ["libs/schema/src/*"],
      "@prodgenie/libs/server-services": ["libs/server-services/src/index.ts"],
      "@prodgenie/libs/server-services/*": ["libs/server-services/src/*"],
      "@prodgenie/libs/shared-utils": ["libs/shared-utils/src/index.ts"],
      "@prodgenie/libs/shared-utils/*": ["libs/shared-utils/src/*"],
      "@prodgenie/libs/sse": ["libs/sse/src/index.ts"],
      "@prodgenie/libs/sse/*": ["libs/sse/src/*"],
      "@prodgenie/libs/store": ["libs/store/src/index.ts"],
      "@prodgenie/libs/store/*": ["libs/store/src/*"],
      "@prodgenie/libs/supabase": ["libs/supabase/src/index.ts"],
      "@prodgenie/libs/supabase/*": ["libs/supabase/src/*"],
      "@prodgenie/libs/types": ["libs/types/src/index.ts"],
      "@prodgenie/libs/types/*": ["libs/types/src/*"],
      "@prodgenie/libs/ui": ["libs/ui/src"],
      "@prodgenie/libs/ui/*": ["libs/ui/src/*"],
      "@prodgenie/libs/utils": ["libs/utils/src"],
      "@prodgenie/libs/utils/*": ["libs/utils/src/*"],
      "@prodgenie/libs/worker": ["libs/worker/src/index.ts"],
      "@prodgenie/libs/worker/*": ["libs/worker/src/*"]
    }
  }
}
